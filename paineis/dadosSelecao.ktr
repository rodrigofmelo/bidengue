        <jsScript_script>&#47;&#47;Script here


if (opcao_var == &quot;1&quot;) {

	strSelect += &quot;select f.ddmu_id as id, f.ddmu_municipio as descricao &quot;;
	strSelect += &quot;from dengue.dengue_d_municipios f, &quot;;
	strSelect += &quot;     ( select distinct c.ddmu_id &quot;;
	strSelect += &quot;       from dengue.dengue_f_casos a inner join dengue.dengue_d_tempo b on (a.ddte_id = b.ddte_id) &quot;;
	strSelect += &quot;                                    inner join dengue.dengue_d_municipios c on (a.ddmu_id = c.ddmu_id) &quot;;
	strSelect += &quot;       where ddte_ano = &quot; + ano_var + &quot; &quot;;
	strSelect += &quot;     ) as base &quot;;
	strSelect += &quot;where  f.ddmu_id = base.ddmu_id &quot;;
	strSelect += &quot;order by ddmu_municipio &quot;;

} else if (opcao_var == &quot;2&quot;) {

	strSelect += &quot;select distinct c.ddmu_microregional as id, c.ddmu_microregional as descricao &quot;;
	strSelect += &quot;from dengue.dengue_f_casos a inner join dengue.dengue_d_tempo b on (a.ddte_id = b.ddte_id) &quot;;
	strSelect += &quot;                             inner join dengue.dengue_d_municipios c on (a.ddmu_id = c.ddmu_id) &quot;;
	strSelect += &quot;where ddte_ano = &quot; + ano_var + &quot; &quot;;
	strSelect += &quot;order by c.ddmu_microregional &quot;;

} else if (opcao_var == &quot;3&quot;) {

	strSelect += &quot;select &apos;GOIAS&apos; as id, &apos;GOIAS&apos; as descricao &quot;;

}


